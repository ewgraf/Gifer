<Window x:Class="giferWpf.MainWindow"
        x:Name="window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:giferWpf"
        mc:Ignorable="d"
        AllowsTransparency="True"
        WindowStyle="None"
        Background="Transparent"
        Title="MainWindow" KeyDown="Window_KeyDown">
    <Grid>
		<Image x:Name="pictureBox1"
			   RenderOptions.BitmapScalingMode="NearestNeighbor"
			   Width="256" Height="256" Margin="0 0 0 0"
			   HorizontalAlignment="Left" VerticalAlignment="Top"
			   AllowDrop="True" Drop="pictureBox1_Drop"
			   Stretch="Fill"
			   MouseDown="pictureBox1_MouseDown" MouseMove="pictureBox1_MouseMove" MouseUp="pictureBox1_MouseUp" MouseWheel="pictureBox1_MouseWheel" SizeChanged="pictureBox1_SizeChanged" MouseRightButtonUp="pictureBox1_MouseRightButtonUp"
            />
	</Grid>
</Window>
